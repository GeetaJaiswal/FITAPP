		// ##### Edit Password #####
		var notyf = new Notyf();

		const form = document.getElementById('changePassword')
		form.addEventListener('submit', changePassword)

		async function changePassword(event) {
			event.preventDefault();
			username = $('#adminEmail').val();
			password = $('#adminPassword').val();
			reTypePass = $('#adminRetype').val();
			$.ajax({
				type: 'POST',
				url: '/admin/changePassword',
				data: { username, password, reTypePass },
				success: function (res) {
					if (res.data == 'ok') {
						notyf.success({
							message: 'Password has been changed',
							duration: 2000,
							position: {
								x: 'right',
								y: 'top',
							},
						})
						$('#changePasswordModal').modal('hide');
						$('.modal-backdrop').remove();
					}
					else if (res.data == 'notChange') {
						notyf.error({
							message: 'Something wrong! Try again',
							duration: 2000,
							position: {
								x: 'right',
								y: 'top',
							},
						})
					}
					else if (res.data == 'passNotMatch') {
						notyf.error({
							message: 'Both Passwords must be same',
							duration: 2000,
							position: {
								x: 'right',
								y: 'top',
							},
						})
					}
					else if (res.data == 'cred') {
						notyf.error({
							message: 'Some Error! Try again',
							duration: 2000,
							position: {
								x: 'right',
								y: 'top',
							},
						})
					}
					else {
						notyf.error({
							message: 'Something is wrong',
							duration: 2000,
							position: {
								x: 'right',
								y: 'top',
							},
						})
					}
				}
			})
		}



