	<!-- ######  change Password   ###### -->
	<div class="modal fade" id="changePasswordModal" tabindex="-1" aria-labelledby="changePasswordModalLabel"
		aria-hidden="true">
		<div class="modal-dialog modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="changePasswordModalLabel">Modal title</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body">
					<form id="changePassword">
						<div class="row">
							<div class="col-12">
								<div class="form-group">
									<input type="email" name="adminEmail" id="adminEmail"
										class="dropDark mb-2 form-control shadow-none" value="<%- username %>" disabled
										required>
								</div>
							</div>
							<div class="col-12">
								<div class="form-group">
									<input type="password" name="adminPassword" id="adminPassword"
										class="dropDark mb-2 form-control shadow-none" placeholder="Enter new Password"
										pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}"
										title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters"
										required>
								</div>
							</div>
							<div class="col-12">
								<div class="form-group">
									<input type="password" name="adminRetype" id="adminRetype"
										class="dropDark mb-2 form-control shadow-none" placeholder="Retype Password"
										required>
								</div>
							</div>
						</div>
						<br>
						<div class="modal-footer">
							<input type="submit" class="btn primary-btn shadow-none">
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>




